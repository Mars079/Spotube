{
    "name": "Spotube",
    "version": "1.0",
    "description": "Making adding your favorites songs to spotify easy",
    "permissions": ["tabs", "declarativeContent"],
    "background": {
      "scripts": ["./src/background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "./src/popup.html"
    },
    "manifest_version": 2
}